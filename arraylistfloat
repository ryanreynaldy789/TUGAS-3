public class arraylistfloat {
    public static void main(String[] args) {
     float a=1,b=2,c=3,d=4,e=5,f=6;
     System.out.println("nilai minimum  : "+fungsiminimum(a,b));
     System.out.println("nilai minimum  : "+fungsiminimum(b,c));
     System.out.println("nilai minimum  : "+fungsiminimum(c,f));
    }
            
    public static int fungsiminimum(float nilai1,float nilai2){
       float min;
       if (nilai1<nilai2)
       min=nilai1;
       else
       min=nilai2;
        return (int) min;
        
        
       }

